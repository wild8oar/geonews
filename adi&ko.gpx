    <url>http://www.geocaching.com/seek/cache_details.aspx?guid=e2b55eaa-a015-4b9d-b44a-74ff28b225a0</url>
    <urlname>Dörfer im Oberengadin - Samedan</urlname>
    <sym>Geocache Found</sym>
    <type>Geocache|Unknown Cache</type>
    <groundspeak:cache id="2521788" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Dörfer im Oberengadin - Samedan</groundspeak:name>
      <groundspeak:placed_by>tommiB und aikaonyx</groundspeak:placed_by>
      <groundspeak:owner id="1013824">tommiB</groundspeak:owner>
