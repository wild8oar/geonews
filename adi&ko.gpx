&lt;td width="80" bgcolor="#663300" align="center"&gt;&lt;b&gt;&lt;a target="_blank" href="http://coord.info/%20GC40JKE" style="text-decoration:none;"&gt;&lt;font color="white"&gt;GC40JKE&lt;/font&gt;&lt;/a&gt;&lt;/b&gt;&lt;/td&gt;
