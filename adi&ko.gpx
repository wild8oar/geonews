    <sym>Geocache Found</sym>
    <type>Geocache|Traditional Cache</type>
    <groundspeak:cache id="4130053" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>RW07 Stägeli uf, Stägeli ab Rund w e g</groundspeak:name>
      <groundspeak:placed_by>RundwegWinterthur, coordinatius, prulin</groundspeak:placed_by>
      <groundspeak:owner id="8660408">RundwegWinterthur</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
      <groundspeak:container>Micro</groundspeak:container>
      <groundspeak:attributes>
