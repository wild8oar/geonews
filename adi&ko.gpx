    <groundspeak:cache id="4914619" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Delphin meets duck</groundspeak:name>
      <groundspeak:placed_by>br.finder</groundspeak:placed_by>
      <groundspeak:owner id="4943893">br.finder</groundspeak:owner>
      <groundspeak:type>Unknown Cache</groundspeak:type>
