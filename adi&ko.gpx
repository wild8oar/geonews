        <groundspeak:log id="344799614">
          <groundspeak:date>2013-08-27T19:00:00Z</groundspeak:date>
          <groundspeak:type>Found it</groundspeak:type>
          <groundspeak:finder id="6673913">Adi&amp;Ko</groundspeak:finder>
          <groundspeak:text encoded="False">Nice cache in a residential surrounding. Thanks!

Log time: 10:00</groundspeak:text>
        </groundspeak:log>
      </groundspeak:logs>
      <groundspeak:travelbugs />
    </groundspeak:cache>
  </wpt>
