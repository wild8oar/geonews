    <type>Geocache|Traditional Cache</type>
    <groundspeak:cache id="4071293" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Schloss Kyburg</groundspeak:name>
      <groundspeak:placed_by>rakessler</groundspeak:placed_by>
      <groundspeak:owner id="2247726">rakessler</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
