    <groundspeak:cache id="1427034" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Die Museggmauer</groundspeak:name>
      <groundspeak:placed_by>thomasdominik</groundspeak:placed_by>
      <groundspeak:owner id="1813774">thomasdominik</groundspeak:owner>
