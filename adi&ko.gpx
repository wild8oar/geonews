          <groundspeak:text encoded="False">Erinnerungen, Erinnerungen! Vielen Dank für diesen Cache, war schön, wieder mal hier vorbei zu kommen. </groundspeak:text>
        </groundspeak:log>
      </groundspeak:logs>
      <groundspeak:travelbugs />
    </groundspeak:cache>
