          <groundspeak:date>2016-01-01T15:03:05Z</groundspeak:date>
          <groundspeak:type>Found it</groundspeak:type>
          <groundspeak:finder id="6673913">Adi&amp;Ko</groundspeak:finder>
          <groundspeak:text encoded="False">Wat een grappige vragen! De cache is op een plek waar we al vaak langs gelopen zijn. Bedankt  voor deze mystery!</groundspeak:text>
        </groundspeak:log>
      </groundspeak:logs>
      <groundspeak:travelbugs />
    </groundspeak:cache>
