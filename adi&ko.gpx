    <groundspeak:cache id="3395338" available="False" archived="True" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Der Buchhändler: Level 1</groundspeak:name>
      <groundspeak:placed_by>ValentinMarolf</groundspeak:placed_by>
      <groundspeak:owner id="6179840">ValentinMarolf</groundspeak:owner>
      <groundspeak:type>Unknown Cache</groundspeak:type>
      <groundspeak:container>Small</groundspeak:container>
      <groundspeak:attributes>
