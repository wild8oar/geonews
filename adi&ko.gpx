    <groundspeak:cache id="4146482" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>RW01 Mutprobe Rund w e g</groundspeak:name>
      <groundspeak:placed_by>RundwegWinterthur, coordinatius, prulin</groundspeak:placed_by>
