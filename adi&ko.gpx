    <sym>Geocache Found</sym>
    <type>Geocache|Multi-cache</type>
    <groundspeak:cache id="2683057" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Normen</groundspeak:name>
      <groundspeak:placed_by>whiterussian_ch</groundspeak:placed_by>
      <groundspeak:owner id="3658941">whiterussian_ch</groundspeak:owner>
      <groundspeak:type>Multi-cache</groundspeak:type>
